{
  "name": "crm-pegeus",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npx tsc",
    "start": "NODE_ENV=production node --max-old-space-size=4096 dist/index.js",
    "server": "pm2 reload pm2.config.js --env production || pm2 start pm2.config.js --env production",
    "start:dev": "yarn cache clean && yarn && yarn build && yarn start",
    "dev": "npx tsc --watch & nodemon --max-old-space-size=4096 -q dist/index.js",
    "prepare": "husky install",
    "seed": "node dist/core/database/seed/index.js"
  },
  "author": "Sur Consulting, LLC",
  "license": "ISC",
  "dependencies": {
    "@adminjs/express": "^6.0.0",
    "@adminjs/import-export": "^3.0.0",
    "@adminjs/mongoose": "^4.0.0",
    "@adminjs/themes": "^1.0.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@material-ui/core": "^4.12.4",
    "@mui/material": "^5.14.2",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/node": "*",
    "adminjs": "^7.2.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.0",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "mongoose": "^7.4.2",
    "nodemon": "^1.14.9",
    "openai": "^3.3.0",
    "react-router-dom": "^6.14.2",
    "recharts": "^2.7.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^4.14.1",
    "react-hook-form": "^7.45.2",
    "react-redux": "^8.1.2",
    "redux": "^4.2.1",
    "tslib": "^2.5.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
