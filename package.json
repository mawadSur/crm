  "description": "",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npx tsc",
    "start": "node dist/index.js",
    "server": "pm2 stop all && pm2 delete all &&  pm2 start yarn --interpreter bash --name crm -- start NODE_ENV=production",
    "dev": "npx tsc --watch & nodemon -q dist/index.js",
    "prepare": "husky install",
    "seed": "node dist/core/database/seed/index.js"
  },
  "author": "Sur Consulting, LLC",
  "license": "ISC",
  "dependencies": {
    "@adminjs/express": "^6.0.0",
    "@adminjs/import-export": "^3.0.0",
    "@adminjs/mongoose": "^4.0.0",
    "@adminjs/themes": "^1.0.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@material-ui/core": "^4.12.4",
    "@mui/material": "^5.14.2",
    "@types/node": "*",
    "adminjs": "^7.0.8",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "mongoose": "^7.3.1",
    "nodemon": "^1.14.9",
